{
  "name": "nexa-digital-marketing",
  "description": "Professional digital marketing agency website built with React, Vite, and Tailwind CSS",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "keywords": ["digital-marketing", "react", "vite", "tailwindcss", "seo", "web-agency"],
  "author": "NEXA Digital Marketing Team",
  "license": "MIT",
  "homepage": "https://nexa-digital-marketing.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/nexa-digital-marketing.git"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint . --fix",
    "lint:check": "eslint .",
    "preview": "vite preview",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "clean": "rm -rf dist",
    "build:analyze": "vite build --mode analyze",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@tailwindcss/vite": "^4.1.11",
    "lucide-react": "^0.525.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-error-boundary": "^4.1.2",
    "react-helmet-async": "^2.0.5",
    "tailwindcss": "^4.1.11"
  },
  "devDependencies": {
    "@eslint/js": "^9.30.1",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react": "^4.6.0",
    "daisyui": "^5.0.46",
    "eslint": "^9.30.1",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "globals": "^16.3.0",
    "prettier": "^3.4.2",
    "prettier-plugin-tailwindcss": "^0.6.9",
    "tailwindcss-motion": "^1.1.1",
    "typescript": "^5.7.3",
    "vite": "^7.0.4",
    "vite-bundle-analyzer": "^0.18.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
